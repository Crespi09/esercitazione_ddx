generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model User {
  id                  Int              @id @default(autoincrement())

  email               String           @unique
  hash                String           
  username            String?
  userRefreshToken    String           @unique @default("")

  createdAt           DateTime         @default(now())
  updatedAt           DateTime         @updatedAt
}

